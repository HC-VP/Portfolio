al\"\n    />\n  </section>\n</template>\n\n<script>\nimport ProjectModal from './ProjectModal.vue'; // Assuming you already have a ProjectModal component\n\nexport default {\n  name: 'Projects',\n  data() {\n    return {\n      projects: [\n        {\n          id: 1,\n          title: 'Portfolio Website',\n          description: 'A personal portfolio built using Vue.js',\n          image: '@/assets/portfolio.png'\n        },\n        {\n          id: 2,\n          title: 'Weather App',\n          description: 'A weather app built with JavaScript and API integration.',\n          image: '@/assets/weather.png'\n        },\n        {\n          id: 3,\n          title: 'E-commerce Site',\n          description: 'An e-commerce platform built with Vue and Node.js.',\n          image: '@/assets/ecommerce.png'\n        }\n      ],\n      selectedProject: null,\n      showModal: false\n    };\n  },\n  methods: {\n    viewProject(project) {\n      this.selectedProject = project;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.selectedProject = null;\n    }\n  },\n  components: {\n    ProjectModal\n  }\n}\n</script>\n\n<style scoped>\n.projects-section {\n  text-align: center;\n  padding: 40px;\n}\n\n.projects-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n  margin-top: 20px;\n}\n\n.project-card {\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  text-align: center;\n}\n\n.project-card img {\n  max-width: 100%;\n  height: auto;\n  border-radius: 10px;\n}\n\n.project-card h3 {\n  margin: 15px 0 10px;\n}\n\n.project-card p {\n  font-size: 14px;\n  color: #666;\n}\n\nbutton {\n  background-color: #42b983;\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n  margin-top: 10px;\n}\n\nbutton:hover {\n  background-color: #36a67c;\n}\n</style>\n"],"mappings":"