{"ast":null,"code":"import ProjectModal from './ProjectModal.vue';\nexport default {\n  data() {\n    return {\n      projects: [{\n        id: 1,\n        title: 'CV',\n        image: require('@/assets/img/CV1.png')\n      }, {\n        id: 2,\n        title: 'Cahier des Charges',\n        image: require('@/assets/img/CDC.png')\n      }, {\n        id: 3,\n        title: 'Dynamiser un espace commentaires',\n        image: require('@/assets/img/Dynamiser.png')\n      }],\n      selectedProject: null,\n      showModal: false,\n      formData: {\n        name: '',\n        subject: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    openModal(project) {\n      this.selectedProject = project;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.selectedProject = null;\n    },\n    sendContactForm() {\n      // Handle sending the contact form\n      alert(`Message sent by ${this.formData.name}`);\n    }\n  },\n  components: {\n    ProjectModal\n  }\n};","map":{"version":3,"names":["ProjectModal","data","projects","id","title","image","require","selectedProject","showModal","formData","name","subject","message","methods","openModal","project","closeModal","sendContactForm","alert","components"],"sources":["/Users/hugo/Desktop/RPS/Portfolio/src/components/AppHome.vue"],"sourcesContent":["<template>\n  <section>\n    <h1>Hugo CEMELI</h1>\n    <p>Welcome to my portfolio! I am a fullstack developer specializing in Vue.js.</p>\n    \n    <!-- Projects Section -->\n    <div class=\"projects\">\n      <div v-for=\"project in projects\" :key=\"project.id\" class=\"project-card\" @click=\"openModal(project)\">\n        <img :src=\"project.image\" alt=\"Project Image\" class=\"project-image\"/>\n        <h3>{{ project.title }}</h3>\n      </div>\n    </div>\n\n    <!-- Project Modal -->\n    <ProjectModal v-if=\"showModal\" :project=\"selectedProject\" @close=\"closeModal\"/>\n\n    <!-- Contact Form Section -->\n    <section class=\"contact-form\">\n      <h2>Contact Me</h2>\n      <form @submit.prevent=\"sendContactForm\">\n        <label for=\"name\">Name</label>\n        <input type=\"text\" id=\"name\" v-model=\"formData.name\" required />\n\n        <label for=\"subject\">Subject</label>\n        <input type=\"text\" id=\"subject\" v-model=\"formData.subject\" required />\n\n        <label for=\"message\">Message</label>\n        <textarea id=\"message\" v-model=\"formData.message\" required></textarea>\n\n        <button type=\"submit\">Send Message</button>\n      </form>\n    </section>\n  </section>\n</template>\n\n<script>\nimport ProjectModal from './ProjectModal.vue';\n\nexport default {\n  data() {\n    return {\n      projects: [\n        { id: 1, title: 'CV', image: require('@/assets/img/CV1.png') },\n        { id: 2, title: 'Cahier des Charges', image: require('@/assets/img/CDC.png') },\n        { id: 3, title: 'Dynamiser un espace commentaires', image: require('@/assets/img/Dynamiser.png') }\n      ],\n      selectedProject: null,\n      showModal: false,\n      formData: {\n        name: '',\n        subject: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    openModal(project) {\n      this.selectedProject = project;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.selectedProject = null;\n    },\n    sendContactForm() {\n      // Handle sending the contact form\n      alert(`Message sent by ${this.formData.name}`);\n    }\n  },\n  components: {\n    ProjectModal\n  }\n}\n</script>\n\n<style scoped>\n.projects {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n  gap: 20px;\n  margin-top: 20px;\n}\n\n.project-card {\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  text-align: center;\n  cursor: pointer;\n  transition: transform 0.3s ease;\n}\n\n.project-card:hover {\n  transform: scale(1.05);\n}\n\n.project-image {\n  max-width: 100%;\n  height: auto;\n  max-height: 150px;\n  border-radius: 10px;\n}\n\n.project-card h3 {\n  margin-top: 15px;\n  font-size: 18px;\n}\n\n.contact-form {\n  margin-top: 40px;\n  padding: 20px;\n  background-color: #f9f9f9;\n  border-radius: 10px;\n}\n\n.contact-form h2 {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.contact-form label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\n.contact-form input, .contact-form textarea {\n  width: 100%;\n  padding: 10px;\n  margin-bottom: 15px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\n\n.contact-form button {\n  background-color: #42b983;\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.contact-form button:hover {\n  background-color: #36a67c;\n}\n</style>\n"],"mappings":"AAoCA,OAAOA,YAAW,MAAO,oBAAoB;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAEC,OAAO,CAAC,sBAAsB;MAAE,CAAC,EAC9D;QAAEH,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,oBAAoB;QAAEC,KAAK,EAAEC,OAAO,CAAC,sBAAsB;MAAE,CAAC,EAC9E;QAAEH,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,kCAAkC;QAAEC,KAAK,EAAEC,OAAO,CAAC,4BAA4B;MAAE,EAClG;MACDC,eAAe,EAAE,IAAI;MACrBC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,OAAO,EAAE;MACjB,IAAI,CAACR,eAAc,GAAIQ,OAAO;MAC9B,IAAI,CAACP,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDQ,UAAUA,CAAA,EAAG;MACX,IAAI,CAACR,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACD,eAAc,GAAI,IAAI;IAC7B,CAAC;IACDU,eAAeA,CAAA,EAAG;MAChB;MACAC,KAAK,CAAC,mBAAmB,IAAI,CAACT,QAAQ,CAACC,IAAI,EAAE,CAAC;IAChD;EACF,CAAC;EACDS,UAAU,EAAE;IACVnB;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}